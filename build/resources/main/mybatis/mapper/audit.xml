<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.dao.AuditDAO">
    <insert id="insertAudit" parameterType="Audit">
        INSERT INTO AUDIT(USER_ID)
        VALUES (#{userId})
    </insert>

    <select id="selectAuditList" parameterType="Audit" resultType="Audit">
        SELECT *
        FROM AUDIT
        WHERE 1=1
        <if test="${userId != null and userId != ''}">
            AND USER_ID = #{userId}
        </if>
        <if test="${date != null and date != ''}">
            AND DATE = #{date}
        </if>
    </select>
</mapper>